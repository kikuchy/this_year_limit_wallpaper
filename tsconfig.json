{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": [
            "esnext"
        ],
        "moduleResolution": "node",
        "strict": true,
        "allowJs": true,
        "checkJs": false,
        "outDir": "dist",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "types": [
            "@cloudflare/workers-types",
            "node"
        ]
    },
    "include": [
        "src/**/*.ts"
    ]
}